<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
 
    <item android:state_pressed="false" >
        <shape
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
	        <solid 
	            android:color="#b0c4de"/>
	        <corners
                android:radius="5dp" />
	        <stroke
                android:width="0.5dp"
                android:color="#e0e0e0" />
        </shape>
    </item>
</selector>


